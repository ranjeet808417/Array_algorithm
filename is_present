#include <iostream>
using namespace std;

bool ispresent(int arr[],int n,int x){
for(int i=0;i<n;i++){
        if(arr[i]==x){
                return true;
        }
}
return false;
}


int main() {
    
int n;
cin>>n;
int *arr=new int[n];
for(int i=0;i<n;i++){
        cin>>arr[i];
}
int x;
cin>>x;
cout<<ispresent(arr,n,x);
}
